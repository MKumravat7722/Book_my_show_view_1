<h1>Listing Books</h1>

<table>
  <thead>
    <tr>
      <th>Title</th>
      <th>Content</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @users.each do |book| %>
      <tr>
        <td><%= user.name %></td>
        <td><%= book.email %></td>
        
      </tr>
    <% end %>
  </tbody>
</table>

<br>
